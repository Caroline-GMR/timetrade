<section class="services">
  <h2>Services</h2>
  <ul>
    {{#each services}}
    <a href="/services/{{this.id}}">
      <div class="card-container" style="background-image:url({{this.url}});background-size:100%">
        <li>
         {{!-- <img src="{{this.url}}" alt="{{this.name}}"> --}}
          <p class="name">{{this.name}}</p>
          <p class="category">{{this.category}}</p>
          <img src="../images/hourglass.png" class="cost-icon" alt="time"><p class="time">{{this.time}}</p>
          
          {{#each this.owner}}
            <ul class="owner">{{this.username}}</ul>
          {{/each}}
        </li>
      </div>
    </a>
    {{/each}}
  </ul>

</section>